  "license": "MIT",
  "bugs": {
    "url": "https://github.com/progressive-web-components/viewer/issues"
  },
  "homepage": "https://github.com/progressive-web-components/viewer#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "prettier": "^1.4.1",
    "webpack": "^2.6.1"
  },
  "dependencies": {
    "d_js": "^1.9.0",
    "unidragger": "^2.2.2"
